#include<iostream>
using namespace std;
int fibonacci(int n) {
    if (n==1 || n==2) {
        return 1;
    } else if (n==0)  {
        return 0;
    } else {
        return fibonacci(n-2)+fibonacci(n-1);
    }
}
int main(void){
    int fib;
    cout<<"Ingrese un nÃºmero: ";
    cin>>fib;
    cout<<"Fibonacci: "<<fibonacci(fib)<<endl;
    return 0;
}
